{
  "name": "bitpay-card",
  "version": "0.1.0",
  "description": "A `MEAN` full stack single page app",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "preinstall": "npm i typings -g",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "node ./server.js",
    "nodemon": "nodemon ./server.js && concurrently \"npm run tsc\" \"npm run tsc:w\"",
    "build": "node bundler.js --prod"
  },
